{
  "name": "drupal/swagger",
  "type": "drupal-module",
  "description": "Provides OpenAPI compliant documentation of Drupal REST API",
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "homepage": "https://www.drupal.org/project/swagger",
  "minimum-stability": "dev",
  "support": {
    "issues": "http://drupal.org/project/issues/swagger",
    "source": "http://cgit.drupalcode.org/swagger"
  },
  "require": {
    "swagger-api/swagger-ui": "2.2.10"
  },
  "repositories": [
    {
      "type": "package",
      "package":{
        "name": "swagger-api/swagger-ui",
        "version": "2.2.10",
        "type": "drupal-library",
        "dist"    : {
          "url": "https://github.com/swagger-api/swagger-ui/archive/v2.2.10.zip",
          "type": "zip"
        }
      }
    }
  ]
}
